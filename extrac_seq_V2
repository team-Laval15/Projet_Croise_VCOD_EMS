import requests

BASE_URL = "http://serveur"
DATASET = "sequences"
COUNTRY = "FR"
TRIMESTER = "1987Q1"

URL = f"{BASE_URL}/archived/{DATASET}/{COUNTRY}/{TRIMESTER}"

proxies = None  # mettre ton proxy ici si besoin

try:
    response = requests.get(URL, timeout=30, proxies=proxies)
    response.raise_for_status()
except requests.exceptions.RequestException as e:
    print("Erreur connexion :", e)
    exit()

with open("sequences_1987Q1_FR.fasta", "w") as f:
    f.write(response.text)

print("Téléchargement terminé")

